{
  "rules": {
    "no-unused-vars": 0,
    "func-names": 0,
    "no-console": 0,
    "vars-on-top": 0,
    "no-use-before-define": 0,
    "no-restricted-syntax":0,
    "guard-for-in": 0,
    "consistent-return":0,
    "no-plusplus": 0,
    "no-param-reassign":0
  },
  "env": {
    "mocha": true
  },
  "parserOptions": {
    "ecmaVersion": 6
  }
}

